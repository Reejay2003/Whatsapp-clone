{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      }
    ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://whatsapp-clone-waxc.onrender.com/api/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ]
  }